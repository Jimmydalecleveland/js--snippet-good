{
  "console log": {
    "prefix": "cl",
    "body": "console.log($1)"
  },
  "console log with color": {
    "prefix": "clc",
    "body": "console.log('%c$1', 'color: ${2:cornflowerblue};'$3)"
  },
  "console log group": {
    "prefix": "clg",
    "body": [
      "console.group('$1')",
      "\tconsole.log($2)",
      "console.groupEnd('$1')"
    ]
  },
