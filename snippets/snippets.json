      "import ${1:${TM_FILENAME/\\..*//}} from '${3:.}'",
      "",
      "describe('$1', () => {",
      "\ttest('$1 should render', () => {",
      "\t\tconst { container } = render(",
      "\t\t\t<$1$4>$5</$1>",
      "\t\t)",
      "",
      "\t\texpect(container).toMatchSnapshot()",
      "\t})",
      "})",
      ""
    ],
    "description": "React Testing Library Bootstrap file with snapshot"
